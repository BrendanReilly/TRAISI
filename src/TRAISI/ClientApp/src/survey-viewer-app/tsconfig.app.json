{
	"extends": "../../tsconfig.json",
	"references": [
		{ "path": "../admin-app"}
	],
	"compilerOptions": {
		"outDir": "../out-tsc/app",
		"baseUrl": "./",
		"module": "es2015",
		"types": ["node"],
		"paths": {
			"traisi-question-sdk": [
				"../../../../TRAISI.SDK/Module/"
			],
			"admin-app": [
				"../admin-app/app/"
			]
		}
