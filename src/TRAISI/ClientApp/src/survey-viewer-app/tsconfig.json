{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"lib": ["es2017", "dom", "esnext"],
		"rootDirs": ["../shared/services"],
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"declaration": false,
		"moduleResolution": "node",
		"outDir": "../out-tsc/app",
		"baseUrl": ".",
		"target": "es6",
		"module": "es6",
		"types": ["node"],
		"paths": {
			"traisi-sdk/*": ["../../../../TRAISI.SDK/Client/src/*"],
			"traisi-question-sdk": ["../../../../TRAISI.SDK/Client/src/"],
			"shared/*": ["../shared/*"]
		}
	},
	"include": ["**/*.ts"],
	"exclude": ["src/test.ts", "../admin-app/**/*", "**/*.spec.ts"]
}
