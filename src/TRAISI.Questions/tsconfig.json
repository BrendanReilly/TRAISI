{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "target": "es5",
    "module": "amd",
    "outDir": "dist",
    "lib": [
      "es2016",
      "dom"
    ],
    "baseUrl": "src",
    "paths": {
      "traisi-sdk/*": [
        "../../TRAISI.SDK/Module/src/*"
      ],
      "traisi-question-sdk": [
        "../../TRAISI.SDK/Module/src"
      ]
    }
  },
  "exclude": [
    "dist/**/*",
    "node_modules/**/*"
  ]
}