{


	"compilerOptions": {
		"lib": [
			"es2017",
			"dom"
		],
		"rootDirs": ["../shared/services"],
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"declaration": false,
		"moduleResolution": "node",
		"outDir": "../out-tsc/app",
		"baseUrl": ".",
		"target": "es5",
		"module": "es2015",
		"types": ["node"],
		"paths": {
			"traisi-sdk/*": [
			  "../../../../TRAISI.SDK/Module/src/*"
			],
			"traisi-question-sdk": [
			  "../../../../TRAISI.SDK/Module/src/*"
			]
		}
	},
	"include": [
		"../shared/**/*.ts",
		"./**/*.ts"
	],
	"exclude": [
		"src/test.ts",
		"../admin-app/**/*.ts",
		"../admin-app/**/*.spec.ts",
		"**/*.spec.ts"
	]
}
