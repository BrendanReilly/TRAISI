{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"cli": {
		"warnings": {
			"typescriptMismatch": false
		}
	},
	"defaultProject": "traisi-admin",
	"newProjectRoot": "projects",
	"projects": {
		"traisi-admin": {
			"root": "",
			"sourceRoot": "src/admin-app",
			"projectType": "application",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/admin",
						"index": "src/admin-app/index.html",
						"main": "src/admin-app/main.ts",
						"tsConfig": "src/admin-app/tsconfig.app.json",
						"progress": false,
						"baseHref": "/admin/",
						"deployUrl": "/admin/",
						"showCircularDependencies": false,
						"polyfills": "src/admin-app/polyfills.ts",
						"assets": [
							"src/admin-app/assets",
							{
								"glob": "**/*",
								"input": "node_modules/ngx-monaco-editor/assets/monaco",
								"output": "src/admin-app/assets/monaco/"
							}
						],
						"styles": [
							"src/admin-app/scss/application.scss",
							"node_modules/bootstrap/dist/css/bootstrap.css",
							"node_modules/animate.css/animate.css",
							"node_modules/awesome-bootstrap-checkbox/awesome-bootstrap-checkbox.css",
							"node_modules/bootstrap-select/dist/css/bootstrap-select.css",
							"node_modules/bootstrap-slider/dist/css/bootstrap-slider.min.css",
							"node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
							"node_modules/ng2-toasty/bundles/style-bootstrap.css",
							"node_modules/@swimlane/ngx-datatable/release/assets/icons.css",
							"node_modules/@swimlane/ngx-datatable/release/themes/bootstrap.css",
							"src/admin-app/scss/alertify.core.css",
							"src/admin-app/scss/alertify.bootstrap.css",
							"node_modules/dropzone/dist/dropzone.css",
							"node_modules/froala-editor/css/froala_editor.pkgd.min.css",
							"node_modules/froala-editor/css/froala_style.min.css",
							"node_modules/mapbox-gl/dist/mapbox-gl.css"
						],
						"scripts": [
							"node_modules/jquery/dist/jquery.js",
							"node_modules/parsleyjs/dist/parsley.js",
							"node_modules/popper.js/dist/umd/popper.js",
							"node_modules/bootstrap/dist/js/bootstrap.js",
							"node_modules/hammerjs/hammer.js",
							"node_modules/bootstrap-toggle/js/bootstrap-toggle.js",
							"node_modules/bootstrap-select/dist/js/bootstrap-select.js",
							"node_modules/bootstrap-slider/dist/bootstrap-slider.min.js",
							"src/admin-app/assets/scripts/alertify.js",
							"node_modules/select2/dist/js/select2.min.js",
							"node_modules/systemjs/dist/system.js",
							"node_modules/froala-editor/js/froala_editor.pkgd.min.js",
							"node_modules/echarts/dist/echarts-en.min.js",
							"node_modules/echarts-gl/dist/echarts-gl.min.js"
						]
					},
					"configurations": {
						"production": {
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": false,
							"fileReplacements": [
								{
									"replace": "src/admin-app/environments/environment.ts",
									"with": "src/admin-app/environments/environment.prod.ts"
								}
							]
						},
						"serve": {
							"extractCss": true,
							"namedChunks": true,
							"vendorChunk": true,
							"commonChunk": true,
							"verbose": true
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "traisi-admin:build:serve",
						"port": 3000
					},
					"configurations": {
						"production": {
							"browserTarget": "traisi-admin:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "traisi-admin:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/e2e/test.ts",
						"karmaConfig": "src/e2e/karma.conf.js",
						"tsConfig": "src/e2e/tsconfig.spec.json",
						"polyfills": "src/admin-app/polyfills.ts",
						"styles": [
							"src/admin-app/scss/application.scss",
							"node_modules/animate.css/animate.css",
							"node_modules/awesome-bootstrap-checkbox/awesome-bootstrap-checkbox.css",
							"node_modules/bootstrap-select/dist/css/bootstrap-select.css",
							"node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
							"node_modules/ng2-toasty/bundles/style-bootstrap.css",
							"node_modules/@swimlane/ngx-datatable/release/assets/icons.css",
							"node_modules/@swimlane/ngx-datatable/release/themes/bootstrap.css",
							"src/admin-app/scss/alertify.core.css",
							"src/admin-app/scss/alertify.bootstrap.css",
							"node_modules/dropzone/dist/dropzone.css"
						],
						"scripts": [
							"node_modules/jquery/dist/jquery.js",
							"node_modules/parsleyjs/dist/parsley.js",
							"node_modules/popper.js/dist/umd/popper.js",
							"node_modules/bootstrap/dist/js/bootstrap.js",
							"node_modules/hammerjs/hammer.js",
							"node_modules/bootstrap-toggle/js/bootstrap-toggle.js",
							"node_modules/bootstrap-select/dist/js/bootstrap-select.js",
							"src/admin-app/assets/scripts/alertify.js",
							"node_modules/select2/dist/js/select2.min.js",
							"node_modules/systemjs/dist/system.js"
						],
						"assets": ["src/admin-app/assets"]
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": ["src/admin-app/tsconfig.app.json"],
						"exclude": ["**/node_modules/**"]
					}
				}
			}
		},
		"traisi-admin-e2e": {
			"root": "",
			"sourceRoot": "src/admin-app-e2e",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "src/e2e/protractor.conf.js",
						"devServerTarget": "traisi:serve"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [],
						"exclude": ["**/node_modules/**"]
					}
				}
			}
		},
		"traisi-survey-viewer-app": {
			"root": "src/survey-viewer-app/",
			"sourceRoot": "src/survey-viewer-app/",
			"projectType": "application",
			"prefix": "traisi",
			"schematics": {},
			"architect": {
				"build": {

					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"deployUrl": "/survey/",
						"baseHref": "/survey/",
						"progress": false,
						"outputPath": "dist/survey",
						"index": "src/survey-viewer-app/index.html",
						"main": "src/survey-viewer-app/main.ts",
						"polyfills": "src/survey-viewer-app/polyfills.ts",
						"tsConfig": "src/survey-viewer-app/tsconfig.app.json",
						"assets": [
							"src/survey-viewer-app/favicon.ico",
							"src/survey-viewer-app/assets"
						],
						"styles": ["src/survey-viewer-app/styles.css"],
						"scripts": []
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "src/survey-viewer-app/environments/environment.ts",
									"with": "src/survey-viewer-app/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true
						},
						"serve": {
							"extractCss": true,
							"namedChunks": true,
							"vendorChunk": true,
							"commonChunk": true,
							"verbose": true
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "traisi-survey-viewer-app:build:serve"
					},
					"configurations": {
						"production": {
							"browserTarget": "traisi-survey-viewer-app:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "traisi-survey-viewer-app:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/survey-viewer-app/src/test.ts",
						"polyfills": "src/survey-viewer-app/src/polyfills.ts",
						"tsConfig": "src/survey-viewer-app/tsconfig.spec.json",
						"karmaConfig": "src/survey-viewer-app/karma.conf.js",
						"styles": ["src/survey-viewer-app/src/styles.css"],
						"scripts": [],
						"assets": [
							"src/survey-viewer-app/favicon.ico",
							"src/survey-viewer-app/assets"
						]
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"src/survey-viewer-app/tsconfig.app.json",
							"src/survey-viewer-app/tsconfig.spec.json"
						],
						"exclude": ["**/node_modules/**"]
					}
				}
			}
		},
		"traisi-survey-viewer-app-e2e": {
			"root": "src/survey-viewer-app-e2e/",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "src/survey-viewer-app-e2e/protractor.conf.js",
						"devServerTarget": "survey-viewer-app:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "survey-viewer-app:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": "src/survey-viewer-app-e2e/tsconfig.e2e.json",
						"exclude": ["**/node_modules/**"]
					}
				}
			}
		}
	},
	"schematics": {
		"@schematics/angular:component": {
			"prefix": "app",
			"styleext": "scss"
		},
		"@schematics/angular:directive": {
			"prefix": "app"
		}
	},
	"version": 1
}
